{
    "name": "react-webpack-template",
    "version": "1.0.0",
    "description": "a react webpack project",
    "main": "index.js",
    "scripts": {
        "start": "better-npm-run start:dll && better-npm-run start:dev",
        "build": "better-npm-run build",
        "lint": "tslint --project tsconfig.json"
    },
    "betterScripts": {
        "start:dll": {
            "command": "npx webpack --config ./config/dll.conf.js",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "start:dev": {
            "command": "npx webpack-dashboard -- node --harmony ./config/dev.conf.js",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "build": {
            "command": "npx webpack --config ./config/base.conf.js",
            "env": {
                "NODE_ENV": "production"
            }
        }
    },
    "babel": {
        "extends": "./config/tools/.babelrc"
    },
    "repository": "git@github.com:MiRinZhang/react-boilerplate.git",
    "author": "MiRinZhang <noodles@yabing.me>",
    "license": "MIT",
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.0",
        "babel-plugin-import": "^1.2.1",
        "babel-plugin-react-transform": "^2.0.2",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-regenerator": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-react-optimize": "^1.0.1",
        "babel-preset-stage-3": "^6.24.1",
        "babel-runtime": "^6.23.0",
        "better-npm-run": "^0.1.0",
        "clean-webpack-plugin": "^0.1.16",
        "cross-env": "^5.0.1",
        "css-loader": "^0.28.4",
        "extract-text-webpack-plugin": "^2.1.2",
        "fork-ts-checker-webpack-plugin": "^0.2.4",
        "html-webpack-plugin": "^2.29.0",
        "node-sass": "^4.5.3",
        "postcss-cssnext": "^2.11.0",
        "postcss-loader": "^2.0.6",
        "react-transform-catch-errors": "^1.0.2",
        "react-transform-hmr": "^1.0.4",
        "redbox-react": "^1.4.2",
        "sass-loader": "^6.0.6",
        "source-map-loader": "^0.2.1",
        "style-loader": "^0.18.2",
        "ts-loader": "^2.2.2",
        "tslint": "^5.5.0",
        "tslint-react": "^3.0.0",
        "typescript": "^2.4.1",
        "webpack": "^3.0.0",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-chunk-hash": "^0.4.0",
        "webpack-dashboard": "^0.4.0",
        "webpack-dev-server": "^2.5.0",
        "webpack-notifier": "^1.5.0"
    },
    "dependencies": {
        "@types/node": "^8.0.9",
        "@types/react": "^15.0.32",
        "@types/react-dom": "^15.5.1",
        "@types/react-loadable": "^4.0.2",
        "@types/react-redux": "^4.4.45",
        "@types/react-router": "^4.0.12",
        "@types/react-router-dom": "^4.0.6",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-loadable": "^4.0.3",
        "react-redux": "^5.0.5",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1",
        "redux": "^3.7.1",
        "whatwg-fetch": "^2.0.3"
    }
}
